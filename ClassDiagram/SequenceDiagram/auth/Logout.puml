@startuml Logout (GET)

header Massage Reservation Sequence Diagram: Logout
footer CEDT Chulalongkorn University
title "Logout (GET)"

participant "User" as user
participant "<<javaScript>>\n:server" as server
participant "<<router>>\n:auth" as routerAuths
participant "<<controllers>>\n:auth" as controllersAuths

user->server ++:req.post('/api/v1/auth/logout')
server->routerAuths ++:app.use('/api/v1/auth',auth)
routerAuths -> controllersAuths ++:logout()

controllersAuths->user --:response, clear cookie

@enduml
